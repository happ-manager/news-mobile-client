{
	"expo": {
		"name": "Happ",
		"slug": "happ",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/images/logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "com.happ-admin.happ",
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/logo.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.happadmin.happ"
		},
		"web": {
			"favicon": "./assets/favicon.ico"
		},
		"extra": {
			"eas": {
				"projectId": "4872d468-b78e-4497-9a39-0115b0584990"
			}
		},
		"owner": "happ-admin",
		"plugins": ["expo-router", "expo-font"],
		"scheme": "happ-scheme"
	}
}
